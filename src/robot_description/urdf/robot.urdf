<?xml version="1.0" ?>

<robot name="arm">

  <gazebo>
    <plugin filename="libgazebo_ros_control.so" name="gazebo_ros_control">
      <robotNamespace>ros_control_tester</robotNamespace>
    </plugin>
  </gazebo>

  <material name="orange">
    <color rgba="1.00 0.67 0.02 1.00"/>
  </material>
  <material name="gray">
    <color rgba="0.80 0.80 0.80 1.00"/>
  </material>
  
  <link name="world"/>
  
  <joint name="world_foot_joint" type="fixed">
    <parent link="world"/>
    <child link="toe"/>
  </joint>

  <joint name="toe_foot_joint" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.03"/>
    <parent link="toe"/>
    <child link="foot"/>
    <axis xyz="0 0 1"/>
    <limit effort="40.0" lower="-3.14159265359" upper="200.0" velocity="1000.0"/>
  </joint>

  <transmission name="trans1">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="toe_foot_joint">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="toe_foot_actuator">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
   </actuator>
  </transmission>

  <link name="toe">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.015"/>
      <geometry>
        <box size="0.02 0.02 0.03"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.015"/>
      <geometry>
        <box size="0.02 0.02 0.03"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.015"/>
      <mass value="1"/>
      <inertia ixx="0.00010833333333333333" ixy="0" ixz="0" iyy="0.00010833333333333333" iyz="0" izz="6.666666666666667e-05"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.03"/>
      <geometry>
        <cylinder length="0.025" radius="0.018"/>
      </geometry>
      <material name="gray"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.03"/>
      <geometry>
        <cylinder length="0.025" radius="0.018"/>
      </geometry>
    </collision>
  </link>

  <gazebo reference="toe">
    <material>Gazebo/Blue</material>
  </gazebo>

  <joint name="foot_leg_joint" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.04"/>
    <parent link="foot"/>
    <child link="leg"/>
    <axis xyz="1 0 0"/>
    <limit effort="40.0" lower="-3.14159265359" upper="3.14159265359" velocity="1.0"/>
  </joint>
  
  <transmission name="trans2">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="foot_leg_joint">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="foot_leg_actuator">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
   </actuator>
  </transmission>

  <link name="foot">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.02"/>
      <geometry>
        <box size="0.02 0.02 0.04"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.02"/>
      <geometry>
        <box size="0.02 0.02 0.04"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.02"/>
      <mass value="1"/>
      <inertia ixx="0.00016666666666666666" ixy="0" ixz="0" iyy="0.00016666666666666666" iyz="0" izz="6.666666666666667e-05"/>
    </inertial>
    <visual>
      <origin rpy="0 1.570796326795 0" xyz="0 0 0.04"/>
      <geometry>
        <cylinder length="0.025" radius="0.018"/>
      </geometry>
      <material name="gray"/>
    </visual>
    <collision>
      <origin rpy="0 1.570796326795 0" xyz="0 0 0.04"/>
      <geometry>
        <cylinder length="0.025" radius="0.018"/>
      </geometry>
    </collision>
  </link>
  <gazebo reference="foot">
    <material>Gazebo/Blue</material>
  </gazebo>

  <joint name="leg_arm_joint" type="revolute">
    <origin rpy="0 0 0" xyz="0 0 0.4"/>
    <parent link="leg"/>
    <child link="arm"/>
    <axis xyz="1 0 0"/>
    <limit effort="40.0" lower="-3.14159265359" upper="3.14159265359" velocity="1.0"/>
  </joint>
  
  <transmission name="trans3">
    <type>transmission_interface/SimpleTransmission</type>
    <joint name="leg_arm_joint">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
    </joint>
    <actuator name="leg_arm_actuator">
      <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
      <mechanicalReduction>1</mechanicalReduction>
   </actuator>
  </transmission>

  <link name="leg">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.2"/>
      <geometry>
        <box size="0.02 0.02 0.4"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.2"/>
      <geometry>
        <box size="0.02 0.02 0.4"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.2"/>
      <mass value="1"/>
      <inertia ixx="0.013366666666666669" ixy="0" ixz="0" iyy="0.013366666666666669" iyz="0" izz="6.666666666666667e-05"/>
    </inertial>
    <visual>
      <origin rpy="0 1.570796326795 0" xyz="0 0 0.4"/>
      <geometry>
        <cylinder length="0.025" radius="0.018"/>
      </geometry>
      <material name="gray"/>
    </visual>
    <collision>
      <origin rpy="0 1.570796326795 0" xyz="0 0 0.4"/>
      <geometry>
        <cylinder length="0.025" radius="0.018"/>
      </geometry>
    </collision>
  </link>
  <gazebo reference="leg">
    <material>Gazebo/Blue</material>
  </gazebo>

  <link name="arm">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.15"/>
      <geometry>
        <box size="0.02 0.02 0.3"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.15"/>
      <geometry>
        <box size="0.02 0.02 0.3"/>
      </geometry>
    </collision>
    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0.15"/>
      <mass value="1"/>
      <inertia ixx="0.007533333333333333" ixy="0" ixz="0" iyy="0.007533333333333333" iyz="0" izz="6.666666666666667e-05"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.3"/>
      <geometry>
        <cylinder length="0.025" radius="0.018"/>
      </geometry>
      <material name="gray"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.3"/>
      <geometry>
        <cylinder length="0.025" radius="0.018"/>
      </geometry>
    </collision>
  </link>
  <gazebo reference="arm">
    <material>Gazebo/Blue</material>
  </gazebo>
</robot>

